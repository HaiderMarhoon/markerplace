<%- include("../partials/head")%>
    <h1 class="text-center">Marketplace listing</h1>
    <a href="/auth/sign-out">Sign out</a>
    <ul>
        <% foundListings.forEach((listing)=>{%>
            <li>
                <a href="/listings/<%= listing._id %>">
                    <h2>
                        <%= listing.title %>
                    </h2>
                </a>
                <p>
                    <%= listing.description %>
                </p>
                <p>
                    <%= listing.price %> BHD
                </p>
                <img src="<%= listing.image.url %>" class="Listing-img" alt="Image of <%= listing.title %>" />
            </li>
            <%})%>
    </ul>

</body>

</html>